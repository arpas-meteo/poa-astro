---
import Layout from "../../layouts/main.astro";
import RealreteDettaglio from "../../components/Realrete_dettaglio";
import { IRealrete, real_rete_ana } from "../../tipi/real_rete_ana";

export async function getStaticPaths() {
  const realrete = real_rete_ana;
  const selezione = realrete; // .splice(1, 10);
  return selezione.map((realrete: IRealrete) => ({
    params: {
      cod_srv: realrete.cod_srv,
    },
    props: {
      realrete,
    },
  }));
}

const realrete = Astro.props.realrete as IRealrete;
---

<Layout title={realrete.cod_srv}>
  <RealreteDettaglio rete={realrete} />
</Layout>
